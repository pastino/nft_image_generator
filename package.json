{
  "name": "nft-image-generator",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npx tsc && npx pm2 start ecosystem.config.js",
    "dev": "nodemon --exec ts-node ./src/index.ts",
    "build": "rm -rf ./build && tsc",
    "postbuild": "node build/index.js",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js --config ./src/shared/ormconfig.ts",
    "migration:migrate": "yarn typeorm migration:generate -- -n migrations",
    "migration:run": "yarn typeorm migration:run",
    "migration:create": "yarn typeorm migration:create -n",
    "migration:revert": "yarn typeorm migration:revert",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/fluent-ffmpeg": "^2.1.21",
    "@types/lodash": "^4.14.195",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.2.5",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@alch/alchemy-web3": "^1.4.7",
    "alchemy-sdk": "2.6.1",
    "axios": "^1.4.0",
    "bottleneck": "^2.19.5",
    "cors": "^2.8.5",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "file-type": "^18.5.0",
    "fluent-ffmpeg": "^2.1.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "mysql": "2.18.1",
    "sharp": "^0.32.1",
    "typeorm": "0.2.34"
  }
}
